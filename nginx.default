server {
    listen 80;
    listen [::]:80;

    location /frontend-backend-difference/api {
        proxy_pass http://unix:/var/demos/frontend_backend_difference/main.sock;
        proxy_set_header Host $http_host;
        proxy_http_version 1.1;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    }

    location / {
        alias /var/demos/statics;
    }
}
